{
    "version": 2,
    "builds": [
      {
        "src": "users/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Project/package.json",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Project/$1"
      }
    ]
  }
  